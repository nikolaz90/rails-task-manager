<section>
  <h2>edit: <%= @task.title%></h2>
    <%= form_with(model: @task, url: update_path) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <%= form.label :details %>
    <%= form.text_field :details %>
    <%= form.label :completed %>
    <%= form.check_box :completed%>
    <%= form.submit %>
  <% end %>
</section>
